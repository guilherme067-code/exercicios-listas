package application;

import java.util.ArrayList;
import java.util.List;

public class application {

	public static void main(String[] args) {
		List<String> list = new ArrayList<>();

		list.add("Maria");
		list.add("alex");
		list.add("bob");
		list.add("anna");
		list.add(2, "fernando");
		System.out.println(list.size()); 

		list.remove("anna");
		for (String x : list) {
			System.out.println(x);
		}
	}

}
